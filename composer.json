{
  "name": "writecrow/corpus_backend",
  "description": "Drupal is an open source content management platform powering millions of websites and applications.",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Mark Fullmer",
      "email": "mfullmer@gmail.com",
      "homepage": "https://markfullmer.com",
      "role": "Developer"
    }
  ],   
  "require": {
    "composer/installers": "1.5.0",
    "drupal-composer/drupal-scaffold": "2.0.1",
    "drupal/adminimal_theme":"1.3",
    "drupal/console": "1.2.0",
    "drupal/core": "8.5.1",
    "drupal/facets": "1.0-beta2",
    "drupal/features": "3.5",
    "drupal/plupload": "1.0-beta1",
    "drupal/search_api": "1.7",
    "drush/drush": "~8",
    "oomphinc/composer-installers-extender": "1.1.2",
    "rvtraveller/qs-composer-installer": "1.1",
    "webflo/drupal-core-strict": "8.5.1",
    "wikimedia/composer-merge-plugin": "~1.4",
    "writecrow/lorem_gutenberg": "1.1",
    "writecrow/tag_converter": "1.0",
    "writecrow/country_code_converter": "dev-master"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "build-assets": [
      "composer install --optimize-autoloader"
    ]
  },
  "extra": {
    "installer-paths": {
      "core": ["type:drupal-core"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "profiles/contrib/{$name}": ["type:drupal-profile"],
      "themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"],
      "modules/custom/{$name}": ["type:drupal-custom-module"],
      "themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "drupal-scaffold": {
      "excludes": [
        ".csslintrc",
        ".eslintrc",
        ".editorconfig",
        ".eslintignore",
        ".eslintrc.json",
        "web.config"
      ]
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  }
}
